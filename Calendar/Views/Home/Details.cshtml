@*Calendar.Models.ChooseDate*@
@model IEnumerable<ChooseDate> 

@{
    ViewBag.Title = "Calendar details page";
}

<div class="card">
    <div class="card-footer text-center">
        <a asp-controller="home" asp-action="index" class="btn btn-primary">Back</a>
    </div>
</div>

<table>
    <thead>
        <tr>
            <th>Data</th>
            <th>IsChecked</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var day in Model)
        {
        <tr>
            <td>@day.SelectedDay</td>
            <td>@day.IsSelected</td>
        </tr>
        }
    </tbody>
</table>

@section Scripts
{
    <script src="~/js/CustomScript.js"></script>
}